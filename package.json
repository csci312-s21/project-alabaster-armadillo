{
  "name": "project-template",
  "version": "0.1.0",
  "private": true,
  "contributors": [
    "Gretchen Doyle gldoyle@middlebury.edu",
    "Hannah Rigdon hrigdon@middlebury.edu",
    "Alex White acw@middlebury.edu",
    "Kaylen Rivers krivers@middlebury.edu",
    "Yaqi Huang yaqih@middlebury.edu",
    "Sophie Eisenberg seisenberg@middlebury.edu"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "test": "jest",
    "lint": "eslint src",
    "heroku:start": "next start -p $PORT"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "cross-spawn": "^7.0.3",
    "knex": "^0.95.4",
    "material-design-icons": "^3.0.1",
    "next": "10.0.8",
    "next-auth": "^3.18.0",
    "next-connect": "^0.10.1",
    "node-fetch": "^2.6.1",
    "path": "^0.12.7",
    "pg": "^8.6.0",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sqlite": "^4.0.21",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.21.0",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md,json,jsx}": "prettier --write"
  },
  "eslintConfig": {
    "rules": {
      "no-shadow": "off",
      "no-use-before-define": "off",
      "no-undef": "off"
    }
  }
}
